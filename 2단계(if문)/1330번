import java.util.*;
{
    public static void main(String[] args) {
        Scanner sh = new Scanner(System.in);

        int A;
        int B;

        A = sh.nextInt();
        B = sh.nextInt();

        if(A > B){
            System.out.println(">");

        }
        else if (A < B){
            System.out.println("<");
        }
        else {
            System.out.println("==");
        }

    }


}
